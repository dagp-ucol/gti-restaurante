<p id="notice"><%= notice %></p>

<h1>Order Foods</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Ticket</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_foods.each do |order_food| %>
      <tr>
        <td><%= order_food.food_id %></td>
        <td><%= order_food.ticket_id %></td>
        <td><%= link_to 'Show', order_food %></td>
        <td><%= link_to 'Edit', edit_order_food_path(order_food) %></td>
        <td><%= link_to 'Destroy', order_food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Food', new_order_food_path %>
