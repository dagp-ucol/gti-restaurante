<p id="notice"><%= notice %></p>

<h1>Inactive Tickets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inactive_tickets.each do |inactive_ticket| %>
      <tr>
        <td><%= link_to 'Show', inactive_ticket %></td>
        <td><%= link_to 'Edit', edit_inactive_ticket_path(inactive_ticket) %></td>
        <td><%= link_to 'Destroy', inactive_ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inactive Ticket', new_inactive_ticket_path %>
